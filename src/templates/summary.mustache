<!DOCTYPE HTML>
<html>
    <head>
        <title>Summary :: Gorrdpd</title>
        {{> styles.mustache}}
    </head>

    <body>
        <div id="container">
            <h6 id="logo">gorrdpd</h6>
            <h1>Summary</h1>

            {{#metrics}}
                <p class="group"><strong>Group &raquo; {{Group}}</strong></p>
                <ul class="graphs short-graphs">
                    {{#Files}}
                        <li>
                            <a href="/metric/{{Name}}">
                                <img1 src="/graph/all/{{Name}}/{{Writer}}?width=400&amp;height=150"/>
                                {{Title}}
                            </a>
                        </li>
                    {{/Files}}
                </ul>
                <div class="clear"></div>
            {{/metrics}}
        </div>
    </body>
</html>